<?xml version="1.0"?>
<package >
  <metadata>
    <id>Monaco.Editor</id>
    <version>0.10.0-alpha</version>
    <title>Monaco Editor UWP Component</title>
    <authors>Michael A. Hawker</authors>
    <owners>Michael A. Hawker</owners>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/hawkerm/monaco-editor-uwp</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A Windows Runtime Component wrapper around the web-based Monaco Editor</description>
    <releaseNotes>Minor fixes, better .NET Native support.</releaseNotes>
    <copyright>Copyright 2020</copyright>
    <tags>uwp monaco editor controls</tags>
    <dependencies>
      <dependency id="Newtonsoft.Json" version="12.0.3"/>
      <dependency id="Nito.AsyncEx" version="5.0.0"/>
      <dependency id="Microsoft.Web.WebView2" version="0.9.430"/>
      <dependency id="Microsoft.WinUI" version="3.0.0-preview1.200629.2-CI"/>
    </dependencies>
  </metadata>
  <files>
    <!-- WinMd and IntelliSense files -->
    <file src="bin\Release\net5.0\MonacoEditorComponent.pri" target="lib\net5.0"/>
    <file src="bin\Release\net5.0\MonacoEditorComponent.dll" target="lib\net5.0"/>
    <file src="bin\Release\net5.0\MonacoEditorComponent.xml" target="lib\net5.0"/>
    <file src="bin\Release\net5.0\MonacoEditorComponent.xr.xml" target="lib\net5.0\MonacoEditorComponent"/>
    <!-- XAML controls -->
    <!--<file src="Themes\Generic.xaml" target="lib\net5.0\Themes"/>
    <file src="bin\Release\Themes\Generic.xbf" target="lib\net5.0\Themes"/>-->
    <file src="Themes\Generic.xaml" target="lib\net5.0\MonacoEditorComponent\Themes"/>
    <file src="bin\Release\net5.0\Themes\Generic.xbf" target="lib\net5.0\MonacoEditorComponent\Themes"/>
    <!-- Dependencies -->
    <file src="CodeEditor\CodeEditor.html" target="lib\net5.0\MonacoEditorComponent\CodeEditor"/>
    <file src="ts-helpermethods\*.js" target="lib\net5.0\MonacoEditorComponent\ts-helpermethods" />
    <file src="monaco-editor\*" target="lib\net5.0\MonacoEditorComponent\monaco-editor"/>
    <file src="monaco-editor\min\**\*.*" target="lib\net5.0\MonacoEditorComponent\monaco-editor\min"/>
    <!-- Docs -->
    <file src="..\README.md" target="content"/>
    <file src="..\changelog.md" target="content"/>
    <file src="..\LICENSE*" target="content"/>
  </files>
</package>
